{
    "name": "luggsoft/php-common-json",
    "description": "Luggsoft / PHP Common JSON",
    "type": "library",
    "homepage": "https://github.com/luggsoft/php-common-json",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Lugg",
            "email": "dan.lugg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "luggsoft/php-common": "dev-master",
        "luggsoft/php-common-collections": "dev-master",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "composer/composer": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
